  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.14.2",
    "@types/seedrandom": "^3.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {
    "@types/google.maps": "^3.48.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
